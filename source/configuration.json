{
    "saves": 10,
    "actual_games_size": 100,
    "grid_size": 7,
    "search_games_time_limit_seconds": 10,
    "exploration_constant": 2,
    "mini_batch_size": 64,
    "max_epochs": 25,
    "greedy_epsilon": 0.1,
    "dynamic_epsilon": false,
    "features": [
        {
            "type": "onehot_encode_player"
        },
        {
            "type": "onehot_encode_opponent"
        },
        {
            "type": "onehot_encode_empty"
        }
    ],
    "criterion": "cross_entropy",
    "optimizer": {
        "type": "adam",
        "lr": 0.01
    },
    "input_layer": {
        "filters": 32,
        "kernel_size": 3,
        "stride": 1,
        "padding": 1,
        "bias": true
    },
    "hidden_layers": [
        {
            "type": "batch_norm_2d"
        },
        {
            "type": "relu"
        },

        {
            "type": "conv",
            "filters": 64,
            "kernel_size": 2,
            "stride": 1,
            "padding": 1,
            "bias": true
        },
        {
            "type": "batch_norm_2d"
        },
        {
            "type": "relu"
        },

        {
            "type": "conv",
            "filters": 128,
            "kernel_size": 1,
            "stride": 1,
            "padding": 1,
            "bias": true
        },
        {
            "type": "batch_norm_2d"
        },
        {
            "type": "relu"
        },

        {
            "type": "flatten"
        },

        {
            "type": "linear",
            "filters": 128,
            "bias": true
        },
        {
            "type": "relu"
        },

        {
            "type": "linear",
            "filters": 64,
            "bias": true
        },
        {
            "type": "relu"
        }
    ]
}
