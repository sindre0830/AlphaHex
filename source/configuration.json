{
    "save_interval": 5,
    "actual_games_size": 10,
    "game_board_size": 5,
    "search_games_size": 200,
    "mini_batch_size": 32,
    "epochs": 64,
    "criterion": "cross_entropy_loss",
    "optimizer": {
        "type": "adam",
        "lr": 0.0005
    },
    "input_layer": {
        "filters": 16,
        "kernel_size": 3,
        "stride": 1,
        "padding": 1,
        "bias": true
    },
    "hidden_layers": [
        {
            "type": "relu"
        },

        {
            "type": "conv",
            "filters": 32,
            "kernel_size": 3,
            "stride": 1,
            "padding": 1,
            "bias": true
        },
        {
            "type": "relu"
        },
        {
            "type": "max_pool",
            "kernel_size": 2,
            "stride": 1
        },

        {
            "type": "conv",
            "filters": 64,
            "kernel_size": 3,
            "stride": 1,
            "padding": 1,
            "bias": true
        },
        {
            "type": "relu"
        },
        {
            "type": "max_pool",
            "kernel_size": 2,
            "stride": 1
        },

        {
            "type": "flatten"
        },

        {
            "type": "linear",
            "filters": 256,
            "bias": true
        },
        {
            "type": "relu"
        }
    ]
}
