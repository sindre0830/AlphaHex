{
    "save_interval": 5,
    "actual_games_size": 10,
    "game_board_size": 5,
    "search_games_size": 50,
    "mini_batch_size": 32,
    "input_layer": {
        "filters": 32,
        "kernel_size": 4,
        "stride": 1,
        "padding": 0,
        "bias": true
    },
    "hidden_layers": [
        {
            "type": "relu"
        },

        {
            "type": "conv",
            "filters": 32,
            "kernel_size": 2,
            "stride": 1,
            "padding": 0,
            "bias": false
        },
        {
            "type": "relu"
        },
        {
            "type": "dropout",
            "p": 0.25
        },

        {
            "type": "conv",
            "filters": 64,
            "kernel_size": 1,
            "stride": 1,
            "padding": 0,
            "bias": true
        },
        {
            "type": "relu"
        },

        {
            "type": "conv",
            "filters": 64,
            "kernel_size": 1,
            "stride": 1,
            "padding": 0,
            "bias": false
        },
        {
            "type": "relu"
        },
        {
            "type": "dropout",
            "p": 0.25
        },

        {
            "type": "flatten"
        },

        {
            "type": "linear",
            "filters": 256,
            "bias": false
        },
        {
            "type": "relu"
        },

        {
            "type": "linear",
            "filters": 128,
            "bias": false
        },
        {
            "type": "relu"
        },

        {
            "type": "linear",
            "filters": 84,
            "bias": false
        },
        {
            "type": "relu"
        },
        {
            "type": "dropout",
            "p": 0.25
        }
    ]
}
